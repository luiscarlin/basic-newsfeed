{
  "name": "basic-newsfeed",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:luiscarlin/basic-newsfeed.git",
  "author": "Luis Carlin <luiscarlin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "parcel index.html --port 3000 --open",
    "prebuild": "run-s clean",
    "build": "parcel build index.html",
    "lint:check": "eslint --ignore-path .gitignore .",
    "lint:fix": "run-s lint:check -- --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm-s lint:fix"
  },
  "devDependencies": {
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  }
}
